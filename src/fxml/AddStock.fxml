<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane fx:id="mainPane" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AddStockController">
    <children>
        <VBox alignment="CENTER" prefHeight="195.0" prefWidth="271.0">
            <children>
            <TextField fx:id="text" focusTraversable="false" maxWidth="-Infinity" onKeyReleased="#typing" onMouseClicked="#retype" prefHeight="26.0" prefWidth="207.0" promptText="StockExchange name" VBox.vgrow="NEVER" />
            <Label fx:id="error" text="Name already exist" textFill="RED" visible="false">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </Label>
                <Button fx:id="okButton" mnemonicParsing="false" onAction="#ok" prefHeight="26.0" prefWidth="95.0" text="OK">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Button>
            </children>
        </VBox>
    </children>
</Pane>
